<Application
  x:Class="LogGenius.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:modernwpf="http://schemas.modernwpf.com/2019"
  xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <modernwpf:ThemeResources>
        </modernwpf:ThemeResources>
        <modernwpf:XamlControlsResources />
      </ResourceDictionary.MergedDictionaries>
      <sys:Double x:Key="FontSize">12</sys:Double>
      <SolidColorBrush x:Key="DataGridCellFocusVisualPrimaryBrush"/>
      <SolidColorBrush x:Key="DataGridCellFocusVisualSecondaryBrush"/>
      <sys:Double x:Key="ContentDialogMaxWidth">1920</sys:Double>
    </ResourceDictionary>
  </Application.Resources>
</Application>
